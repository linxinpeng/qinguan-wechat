.ent-member__header {
  width: 100%;
}
.ent-member__header image {
  width: 100%;
  object-fit: contain;
  display: block;
}
.ent-member__cont {
  min-height: 20rpx;
  background-color: #1B1410;
  -webkit-transform: translateY(-1rpx);
          transform: translateY(-1rpx);
}
.ent-member__cont-db {
  padding-top: 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 40rpx;
}
.ent-member__title {
  height: 2rpx;
  background-color: #E7CEB1;
  width: 460rpx;
  position: relative;
}
.ent-member__title text {
  position: absolute;
  top: -22rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 265rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #E7CEB1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  background-color: #1B1410;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.ent-member__table {
  margin-top: 60rpx;
  width: 100%;
  background: #FFFFFF;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
}
.ent-member__table-header {
  background: -webkit-linear-gradient(356deg, #FFEBD1 0%, #BE9E6A 100%);
  background: linear-gradient(94deg, #FFEBD1 0%, #BE9E6A 100%);
  -webkit-border-radius: 8rpx 8rpx 0rpx 0rpx;
          border-radius: 8rpx 8rpx 0rpx 0rpx;
}
.ent-member__table-header, .ent-member__row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 70rpx;
  border-bottom: 2rpx solid #E9EAEC;
}
.ent-member__row:last-child {
  border-bottom: none;
}
.ent-member__table-header text, .ent-member__row text {
  width: 24%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 24rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #666666;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  border-right: 2rpx solid #E9EAEC;
}
.ent-member__table-header text:last-child, .ent-member__row text:last-child {
  border-right: none;
}
.ent-member__js {
  width: 100%;
}
.ent-member__js image {
  width: 100%;
}
